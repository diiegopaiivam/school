<!DOCTYPE html>
<html>
  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <title>School</title>
        <!-- Bootstrap core CSS -->
    <link href="http://localhost:3000/templates/site-template/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="http://localhost:3000/templates/site-template/assets/css/ie10-viewport-bug-workaround.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="navbar.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/http://localhost:3000/templates/startbootstrap-sb-admin-2/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application' %>

  </head>
  <body>

    <div class="container">

      <!-- Static navbar -->
      <nav class="navbar navbar-default">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">School</a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav"></ul>
            <%= form_with url: site_search_path, local: true, class:"navbar-form navbar-left", method: :get do |form| %>
              <div class="form-group">
                <%= form.text_field :term, class:"form-control", placeholder:"Digite aqui..." %>
              </div>
              <button type="submit" class="btn btn-default">Pesquisar</button>
            <% end %>
            <ul class="nav navbar-nav navbar-right">
              <% unless profile_signed_in? %>
                <li><%= link_to 'Entrar', new_profile_session_path %></li>
                <li><%= link_to 'Cadastrar-se', new_profile_registration_path %></li>
              <% else%>
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%= current_profile.email %> <span class="caret"></span></a>
                  <ul class="dropdown-menu">
                    <li>
                      <%= link_to profiles__backoffice_welcome_index_path do %>
                          <i class="fa fa-user fa-fw"></i> Perfil
                      <% end %>
                    </li>
                    <li role="separator" class="divider"></li>
                    <li>
                      <%= link_to destroy_profile_session_path, method: :delete do %>
                          <i class="fa fa-sign-out fa-fw"></i> Sair
                      <% end %>
                    </li>
                  </ul>
                </li>
              <% end %>
          </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
      </nav>

      <!-- Main component for a primary marketing message or call to action -->
      <div class="jumbotron">
        <h2>Teste seus conhecimentos</h2>
      </div>
      <%= yield %>
    </div> <!-- /container -->

    <%= javascript_pack_tag 'application' %>
        <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
    <!--[if lt IE 9]><script src="../../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <script src="http://localhost:3000//templates/site-template/docs/assets/js/ie-emulation-modes-warning.js"></script>
    <script src="http://localhost:3000//templates/startbootstrap-sb-admin-2/vendor/jquery/jquery.min.js"></script>
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="templates/site-template/docs/assets/js/vendor/jquery.min.js"><\/script>')</script>
    <script src="http://localhost:3000/templates/site-template/docs/dist/js/bootstrap.min.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="http://localhost:3000/templates/site-template/docs/assets/js/ie10-viewport-bug-workaround.js"></script>

    <script src="http://localhost:3000/js/jquery.bootstrap-growl.min.js"></script>

    <% if notice %>
        <%= javascript_tag do %>
            $.bootstrapGrowl("<%= notice %>", {
                type: 'success', // (null, 'info', 'danger', 'success')
                align: 'right', // ('left', 'right', or 'center')
                allow_dismiss: true, // If true then will display a cross to close the popup.
                stackup_spacing: 10 // spacing between consecutively stacked growls.
            });     
        <% end %>
    <% end %>
  </body>
</html>

