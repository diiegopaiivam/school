<h1>Seja Bem vindo!</h1>
<div class="top_tiles">
    <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
    <div class="tile-stats">
        <div class="icon"><i class="fa fa-comments-o"></i></div>
        <div class="count"><%= @profile_statistic.total_questions %></div>
        <h3>Total</h3>
        <p>Quantidade de questões respondidas.</p>
    </div>
    </div>
    <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
    <div class="tile-stats">
        <div class="icon"><i class="fa fa-caret-square-o-right"></i></div>
        <div class="count"><%= @profile_statistic.right_question %></div>
        <h3>Acertos</h3>
        <p>Quantidade de questões acertadas</p>
    </div>
    </div>
    <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
    <div class="tile-stats">
        <div class="icon"><i class="fa fa-close"></i></div>
        <div class="count"><%= @profile_statistic.wrong_question %></div>
        <h3>Erros</h3>
        <p>Total de questões erradas.</p>
    </div>
    </div>
</div>
